{
  "name": "choco-tube",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "forwardPorts": [5000],
  "postCreateCommand": "pip install -r requirements.txt",
  "postStartCommand": "python app.py",
  "portsAttributes": {
    "5000": {
      "label": "Flask App",
      "onAutoForward": "openPreview"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  }
}
